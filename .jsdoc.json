{
  "docdash": {
    "static": false,
    "sort": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/docdash",
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "monospaceLinks": false
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      "./app",
      "./bin"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  }
}