{
  "name": "pb-for-desktop",
  "appId": "de.sidneys.pb-for-desktop",
  "productName": "PB for Desktop",
  "version": "9.5.0",
  "description": "PB for Desktop is a Pushbullet desktop application for macOS, Windows and Linux",
  "license": "MIT",
  "homepage": "https://sidneys.github.io/pb-for-desktop",
  "author": {
    "name": "sidneys",
    "email": "sidneys.github.io@outlook.com",
    "url": "https://github.com/sidneys"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidneys/pb-for-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/pb-for-desktop/issues"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">= 6.0.0"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "keywords": [
    "pushbullet",
    "pushes",
    "notification",
    "chrome",
    "safari",
    "desktop",
    "electron",
    "mac",
    "macos",
    "osx",
    "windows",
    "linux",
    "ubuntu",
    "native",
    "pushover"
  ],
  "preferGlobal": true,
  "dependencies": {
    "@sidneys/dom-tools": "^1.79.0",
    "@sidneys/electron-build": "^1.104.0",
    "@sidneys/electron-debug-service": "^0.82.0",
    "@sidneys/electron-deploy-github": "^1.88.0",
    "@sidneys/electron-dialog-provider": "^1.84.0",
    "@sidneys/electron-localsetup": "^1.84.0",
    "@sidneys/electron-notification-provider": "^0.84.0",
    "@sidneys/electron-power-service": "^0.82.0",
    "@sidneys/electron-updater-service": "^0.110.0",
    "@sidneys/is-env": "^1.86.0",
    "@sidneys/logger": "^1.92.0",
    "@sidneys/platform-tools": "^1.92.0",
    "@sidneys/releasenotes": "^1.82.0",
    "app-root-path": "^2.2.1",
    "appdirectory": "^0.1.0",
    "auto-launch": "git+https://sidneys@github.com/sidneys/node-auto-launch.git#bugfixes-maintainance",
    "data-uri-to-buffer": "^2.0.1",
    "detect-installed": "^2.0.4",
    "dynamic-throttled-queue": "^1.1.1",
    "electron-editor-context-menu": "^1.1.1",
    "electron-settings": "^3.2.0",
    "electron-updater": "^4.1.2",
    "file-type": "^12.1.0",
    "file-url": "^3.0.0",
    "filesize": "^4.1.2",
    "fs-extra": "^8.1.0",
    "get-youtube-id": "^1.0.1",
    "howler": "^2.1.2",
    "icojs": "^0.13.1",
    "image-downloader": "^3.5.0",
    "jimp": "^0.6.4",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "opn": "^6.0.0",
    "parse-domain": "^2.3.1",
    "read-pkg": "^5.2.0",
    "read-pkg-up": "^6.0.0",
    "shortid": "^2.2.14",
    "simple-reload": "0.0.4",
    "start": "^5.1.0"
  },
  "devDependencies": {
    "@wabson/list-dependencies": "^0.1.0",
    "docdash": "^1.1.1",
    "electron": "6.0.1",
    "eslint": "^6.1.0",
    "jsdoc": "^3.6.3",
    "typescript": "^3.5.3"
  },
  "main": "./app/scripts/main/components/application.js",
  "bin": {
    "pb-for-desktop": "./bin/cli.js"
  },
  "scripts": {
    "build": "node ./node_modules/@sidneys/electron-build/index.js",
    "deploy": "node ./node_modules/@sidneys/electron-deploy-github/index.js",
    "docs": "./node_modules/.bin/jsdoc --configure ./.jsdoc.json",
    "localsetup": "node ./node_modules/@sidneys/electron-localsetup/index.js",
    "releasenotes": "node ./node_modules/@sidneys/releasenotes/index.js",
    "start": "./node_modules/.bin/electron ./app/scripts/main/components/application.js"
  },
  "build": {
    "appId": "de.sidneys.pb-for-desktop",
    "asar": true,
    "asarUnpack": [
      "sounds"
    ],
    "compression": "maximum",
    "directories": {
      "buildResources": "./build/staging",
      "output": "./build/output"
    },
    "files": [
      "!.appveyor.yml",
      "!.babelrc",
      "!.editorconfig",
      "!.eslintignore",
      "!.eslintrc",
      "!.github/**",
      "!.gitignore",
      "!.npmignore",
      "!.travis.yml",
      "!build/**",
      "!gh-pages/**",
      "!resources/**",
      "!website/**"
    ],
    "mac": {
      "target": [
        "dir",
        "dmg",
        "zip"
      ],
      "artifactName": "${name}-${version}-${os}.${ext}",
      "icon": "./icons/mac/icon.icns",
      "category": "public.app-category.utilities",
      "darkModeSupport": true
    },
    "win": {
      "target": [
        "nsis",
        "msi",
        "portable"
      ],
      "artifactName": "${name}-${version}.${ext}",
      "icon": "./icons/win/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "pacman",
        "rpm",
        "snap"
      ],
      "artifactName": "${name}-${version}-${arch}.${ext}",
      "icon": "./icons/linux/png",
      "category": "Utility"
    },
    "dmg": {
      "artifactName": "${name}-${version}.${ext}",
      "background": "./resources/setup/dmg-background.png",
      "icon": "./resources/setup/dmg-icon.icns"
    },
    "nsis": {
      "artifactName": "${name}-${version}-setup.${ext}",
      "deleteAppDataOnUninstall": true,
      "installerHeader": "./resources/setup/nsis-installer-header.bmp",
      "installerHeaderIcon": "./resources/setup/nsis-installer-header-icon.ico",
      "installerIcon": "./resources/setup/nsis-installer-icon.ico",
      "installerSidebar": "./resources/setup/nsis-installer-sidebar.bmp",
      "oneClick": false,
      "runAfterFinish": false
    },
    "productName": "PB for Desktop",
    "releaseInfo": {
      "releaseDate": "2019-08-06T10:52:44.831Z",
      "releaseName": "PB for Desktop",
      "releaseNotes": "## 9.2.9\n\n#### ðŸš¨ Fixes\n\n - fixes multiple isses regarding application auto updates\n\n#### ðŸ‘· Internals\n\n - upgrades internal `node_modules`\n - upgrades external `node_modules`\n\n"
    }
  }
}
